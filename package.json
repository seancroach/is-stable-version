{
  "name": "is-stable-version",
  "version": "1.0.1",
  "description": "Utility function to see if a SemVer version isn't a prerelease.",
  "license": "MIT",
  "repository": "seancroach/is-stable-version",
  "author": "Sean Colin Roach <me@seancroach.dev> (https://seancroach.dev)",
  "main": "build/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "ava": "ava --fail-fast",
    "build": "tsc -p source",
    "clean": "rm -rf build",
    "eslint": "eslint \"**/*.ts\"",
    "format": "npm run prettier:write",
    "lint": "npm run prettier:check && npm run eslint",
    "prettier:check": "prettier --check \"**\"",
    "prettier:write": "prettier --write \"**\"",
    "test": "npm run lint && npm run ava"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "is",
    "release",
    "semver",
    "stable",
    "version"
  ],
  "dependencies": {
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@seancroach/eslint-config-typescript": "^3.0.8",
    "@seancroach/tsconfig": "^2.1.0",
    "@types/semver": "^7.3.4",
    "ava": "^3.15.0",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "files": [
      "test/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@seancroach/eslint-config-typescript"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --check",
    "*.ts": "eslint"
  },
  "prettier": {
    "proseWrap": "never"
  }
}
